#include<stdio.h>
int main(){
    int num,last,rev=0,og;
    scanf("%d",&num);
    og=num;
    while(num!=0){
        last=num%10;
        rev=rev*10+last;
        num=num/10;
    }
    if(og==rev){
        printf("Palindrome");
    }
    else{
        printf("Not Palindrome");
    }
    return 0;
}
