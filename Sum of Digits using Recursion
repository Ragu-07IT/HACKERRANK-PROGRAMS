#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int sumOfDigits(int num){
    if(num==0){
        return 0;
    }
    else{
        return (num%10)+sumOfDigits(num/10);
    }
}
int main() {
    int num;
    scanf("%d",&num);
    printf("%d",sumOfDigits(num));
    return 0;
}
