#include<stdio.h>
#include<string.h>
#include<ctype.h>
int main(){
    char str[100];
    int len,flag=1;
    fgets(str,sizeof(str),stdin);
    for(int i=0;str[i]!='\0';i++){
        str[i]=toupper(str[i]);
    }
    str[strcspn(str,"\n")]='\0';
    len=strlen(str);
    for(int i=0;i<len;i++){
        if(str[i]!=str[len-i-1]){
            flag=0;
            break;
        }
    }
    if(flag)
        printf("Palindrome");
    else
        printf("Not Palindrome");
    return 0;
}
