#include <stdio.h>
#include <math.h>

int isArmstrong(int n) {
    int original = n, sum = 0, digits = 0;
    int temp = n;

    while (temp != 0) {
        digits++;
        temp /= 10;
    }

    temp = n;
    while (temp != 0) {
        int digit = temp % 10;
        sum += pow(digit, digits);
        temp /= 10;
    }

    return (sum == original);
}

int isPrime(int n) {
    if (n <= 1)
        return 0;
    for (int i = 2; i <= sqrt(n); i++) {
        if (n % i == 0)
            return 0;
    }
    return 1;
}

int isPerfect(int n) {
    if (n < 2)
        return 0;
    int sum = 0;
    for (int i = 1; i <= n / 2; i++) {
        if (n % i == 0)
            sum += i;
    }
    return (sum == n);
}

int main() {
    int number;
    scanf("%d", &number);

    if (isArmstrong(number)) {
        printf("Armstrong Number\n");
    } else if (isPrime(number)) {
        printf("Prime Number\n");
    } else if (isPerfect(number)) {
        printf("Perfect Number\n");
    } else {
        printf("None of these\n");
    }

    return 0;
}
