#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int duplicatecount=0;
    for(int i=0;i<n;i++){
        int count=0;
        
        for(int j=0;j<n;j++){
            if(arr[i]==arr[j]){
                count++;
            }
        }
        if(count>1){
            int first=1;
            for(int k=0;k<i;k++){
                if(arr[k]==arr[i]){
                    first=0;
                    break;
                }
            }
            if(first)
                duplicatecount++;
        }
    }
    printf("%d",duplicatecount);
    return 0;
}
