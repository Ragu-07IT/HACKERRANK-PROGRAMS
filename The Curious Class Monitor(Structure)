#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
struct student{
    char name[100];
    int age;
    char grade;
};
int main() {
    struct student s;
    fgets(s.name,sizeof(s.name),stdin);
    scanf("%d",&s.age);
    scanf(" %c",&s.grade);
    
    printf("Name: %s",s.name);
    printf("Age: %d\n",s.age);
    printf("Grade: %c",s.grade);
    return 0;
}
