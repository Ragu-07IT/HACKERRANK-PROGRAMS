#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int *arr1=(int*)calloc(n, sizeof(int));
    if(arr1==NULL){
        return 0;
    }
    for(int i=0;i<n;i++){
        scanf("%d",arr1+i);
    }
    int *arr2=(int*)calloc(n,sizeof(int));
     if(arr2==NULL){
        return 0;
    }
    for(int i=0;i<n;i++){
        scanf("%d",arr2+i);
    }
    for(int i=0;i<n;i++){
        *(arr1+i)=*(arr1+i)+*(arr2+i);
        *(arr2+i)=*(arr1+i)-*(arr2+i);
       *(arr1+i)=*(arr1+i)-*(arr2+i);
    }
    for(int i=0;i<n;i++){
        printf("%d ",*(arr1+i));
    }
    printf("\n");
     for(int i=0;i<n;i++){
        printf("%d ",*(arr2+i));
    }
    return 0;
}
