#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
struct student{
    char name[100];
    int age;
    char grade;
};

int main() {
    int n;
    scanf("%d",&n);
    struct student s[n];
    for(int i=0;i<n;i++){
        scanf("%s",&s[i].name);
        scanf("%d",&s[i].age);
        scanf(" %c",&s[i].grade);
    }
    
    for(int i=0;i<n;i++){
        printf("Student %d:\n",i+1);
        printf("Name: %s\n",s[i].name);
        printf("Age: %d\n",s[i].age);
        printf("Grade: %c\n",s[i].grade);
    }
    return 0;
}
