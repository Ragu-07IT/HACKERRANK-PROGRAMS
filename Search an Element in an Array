#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int val,flag=-1;
    scanf("%d",&val);
    for(int i=0;i<n;i++){
        if(arr[i]==val){
            flag=i;
            break;
        }
    }
    if(flag==-1)
        printf("Element not found");
    else
        printf("Element found at position %d",flag+1);
    return 0;
}
