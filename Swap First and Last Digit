#include<stdio.h>
int main(){
    int num,first,last,middle=0,place=1;
    scanf("%d",&num);
    last=num%10;
    
    first=num;
    while(first>=10){
        first=first/10;
        place=place*10;
    }
    middle=(num%place)/10;
    num=last*place+middle*10+first;
    printf("%d",num);
    return 0;
}
